header{
    position: relative;
    top: 0;
    width: 100%;
    height: 25vw;
    background-color: orange;
}
main{
    padding: 2vw;
    background-color: rgb(237, 237, 237);
    >ul{
        width: 100%;
        border-radius: 2vw;
        overflow: hidden; 
        background-color: rgb(227, 227, 227);
        border: 1px solid rgb(219, 219, 219);  
        li{
            border-bottom: 1px solid rgb(219, 219, 219);
            h3{ 
                line-height: 12.5vw;
                height: 100%;
                padding: 0 3.125vw;
                font-size: 5vw;
                background-color: rgb(237, 237, 237);
                // border-bottom: 1px solid rgb(219, 219, 219);
                span.fa{
                    float: right;
                    line-height: 12.5vw;
                    color: rgb(219, 219, 219);
                }
            }
            ul{ 
                display: none;
                overflow: hidden;
                background-color:rgb(249, 249, 249);
                border-bottom: 2px solid #999;
                li{
                    float: left;
                    width: 33.33%;
                    font-size: 3.75vw; 
                    text-align: center;
                    line-height: 12.5vw;
                    border-bottom: 1px solid rgb(219, 219, 219);
                    border-right: 1px solid rgb(219, 219, 219);
                    &:nth-child(3n){
                      border-right: none;  
                    }
                }
            }
        }
    }
}
footer{
    // position: relative;
    // bottom: 0;
    width: 100%;
    height: 12.5vw;
    background-color: orange;
}