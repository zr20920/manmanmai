header {
  position: relative;
}
header .xy_header {
  width: 100%;
  height: 50px;
  background-color: #ff841d;
  position: relative;
}
header .xy_header .header_logo {
  width: 66%;
  position: absolute;
  top: 10px;
  left: 14px;
}
header .xy_header .header_logo a img {
  width: 185px;
  height: 25px;
}
header .xy_header .header_app {
  position: absolute;
  top: 0;
  right: 0;
}
header .xy_header .header_app a {
  padding: 6px;
}
header .xy_header .header_app a img {
  width: 34px;
  height: 34px;
}
header .xy_searchBar {
  width: 100%;
  height: 50px;
  border-bottom: 2px solid #f1f1f1;
  padding: 10px;
}
header .xy_searchBar .search-inner {
  width: 100%;
  height: 30px;
  position: relative;
}
header .xy_searchBar .search-inner #searchText {
  width: 75vw;
  height: 100%;
  background-color: #f1f1f1;
  border-radius: 15px;
  padding-left: 15px;
  position: absolute;
  left: 0;
}
header .xy_searchBar .search-inner #searchBtn {
  width: 16.8vw;
  height: 100%;
  background-color: #ff841d;
  border-radius: 15px;
  font-size: 4.75vw;
  color: #fff;
  position: absolute;
  right: 0;
}
main.section1 {
  padding: 2vw;
  background-color: #ededed;
}
main.section1 > ul {
  width: 100%;
  border-radius: 2vw;
  overflow: hidden;
  background-color: #e3e3e3;
  border: 1px solid #dbdbdb;
}
main.section1 > ul li {
  border-bottom: 1px solid #dbdbdb;
}
main.section1 > ul li h3 {
  line-height: 12.5vw;
  height: 100%;
  padding: 0 3.125vw;
  font-size: 5vw;
  background-color: #ededed;
}
main.section1 > ul li h3 span.fa {
  float: right;
  line-height: 12.5vw;
  color: #dbdbdb;
}
main.section1 > ul li ul {
  display: none;
  overflow: hidden;
  background-color: #f9f9f9;
  border-bottom: 2px solid #999;
}
main.section1 > ul li ul li {
  float: left;
  width: 33.33%;
  font-size: 3.75vw;
  text-align: center;
  line-height: 12.5vw;
  border-bottom: 1px solid #dbdbdb;
  border-right: 1px solid #dbdbdb;
}
main.section1 > ul li ul li:nth-child(3n) {
  border-right: none;
}
main.section2 {
  display: none;
  margin: 0vw auto;
  width: 100%;
  position: relative;
}
main.section2 .topBar {
  height: 12.5vw;
  padding: 0 2vw;
  background: linear-gradient(to bottom, #fff 0%, #eee 100%);
  border-top: 2px solid #aaa;
}
main.section2 .topBar a {
  height: 100%;
  line-height: 12.5vw;
  float: left;
  margin-right: 1vw;
}
main.section2 .topBar button {
  position: absolute;
  right: 2vw;
  background: linear-gradient(to top, #fff 0%, #ccc 100%);
  border: 1px solid #dbdbdb;
  padding: 0 4vw;
  margin: 1.25vw 0;
  height: 10vw;
  border-radius: 2vw;
}
main.section2 .content ul li {
  height: 30vw;
  border-bottom: 1px solid #dbdbdb;
}
main.section2 .content ul li > div {
  width: 100%;
  margin: 0 auto;
}
main.section2 .content ul li a {
  width: 30%;
  height: 100%;
  float: left;
}
main.section2 .content ul li a img {
  width: 80%;
  margin: 0 auto;
  height: 100%;
}
main.section2 .content ul li .cont {
  padding: 2.5vw 0;
  height: 100%;
}
main.section2 .content ul li .cont p {
  line-height: 6vw;
  max-height: 12vw;
  overflow: hidden;
  padding: 0 2vw;
}
main.section2 .content ul li .cont .price {
  color: red;
}
main.section2 .content ul li .cont span {
  font-size: 3.75vw;
  color: #ccc;
  padding: 0 2vw;
}
main.section2 .pageBar {
  height: 12.5vw;
  padding: 2vw 30vw;
  position: relative;
}
main.section2 .pageBar select {
  display: block;
  margin: 0 auto;
  height: 100%;
  width: 60%;
  font-size: 5vw;
}
main.section2 .pageBar button {
  position: absolute;
  top: 0;
  background: linear-gradient(to top, #eee 0%, #ccc 100%);
  box-shadow: 0 1px 1px 0 #ccc;
  border: 1px solid #dbdbdb;
  padding: 0 4vw;
  margin: 1.5vw 0;
  height: 70%;
  border-radius: 2vw;
}
main.section2 .pageBar button:nth-of-type(1) {
  left: 4vw;
}
main.section2 .pageBar button:nth-of-type(2) {
  right: 4vw;
}
main.section3 {
  display: none;
  margin: 0vw auto;
  width: 100%;
  overflow: hidden;
  position: relative;
}
main.section3 .topBar {
  height: 12.5vw;
  padding: 0 2vw;
  background: linear-gradient(to bottom, #fff 0%, #eee 100%);
  border-top: 2px solid #aaa;
}
main.section3 .topBar a {
  height: 100%;
  line-height: 12.5vw;
  float: left;
  margin-right: 1vw;
}
main.section3 .topBar button {
  position: absolute;
  right: 2vw;
  background: linear-gradient(to top, #fff 0%, #ccc 100%);
  border: 1px solid #dbdbdb;
  padding: 0 4vw;
  margin: 1.25vw 0;
  height: 10vw;
  border-radius: 2vw;
}
main.section3 .content .pro .p_t {
  position: relative;
}
main.section3 .content .pro .p_t h3 {
  text-align: center;
  margin-top: 5vw;
}
main.section3 .content .pro .p_t img {
  width: 50%;
  height: 50%;
  margin: 0 auto;
}
main.section3 .content .pro .p_t span {
  position: absolute;
  bottom: 5vw;
  right: 10vw;
  padding: 2vw 0.8vw;
  font-size: 3.75vw;
  color: #fff;
  border-radius: 50%;
  background-color: #8bc900;
}
main.section3 .content .pro table {
  border-bottom: 1px solid #dbdbdb;
}
main.section3 .content .pro table img {
  width: 50%;
  height: 50%;
  margin: 2vw auto;
}
main.section3 .content .pro > p {
  padding: 3vw;
}
main.section3 .content .comp h3 {
  background: linear-gradient(to top, #eef 0%, #ddd 100%);
  text-align: center;
  height: 12.5vw;
  line-height: 12.5vw;
}
main.section3 .content .comp > div ul li > div {
  color: #c1c1c1;
  font-size: 3.75vw;
  overflow: hidden;
  padding: 3vw;
}
main.section3 .content .comp > div ul li > div span {
  float: left;
}
main.section3 .content .comp > div ul li > div > div {
  float: right;
}
main.section3 .content .comp > div ul li > div > div p {
  text-align: right;
}
main.section3 .content .comp > div ul li > p {
  padding: 0 3vw 3vw;
  border-bottom: 1px solid #dbdbdb;
}
main.section3 .pageBar {
  height: 20vw;
}
main.section3 .pageBar button {
  background: linear-gradient(to top, #eee 0%, #ccc 100%);
  box-shadow: 0 1px 1px 0 #ccc;
  border: 1px solid #dbdbdb;
  margin: 4vw 10%;
  font-size: 5vw;
  height: 50%;
  width: 80%;
  border-radius: 2vw;
}
footer .xy_footer {
  width: 100%;
  height: 122px;
  background-color: #efefef;
  padding-top: 20px;
}
footer .xy_footer .footer_tools {
  height: 32px;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
}
footer .xy_footer .footer_tools a {
  float: left;
  width: 30%;
  height: 100%;
  border-right: 1px solid #c4c4c4;
  line-height: 32px;
  text-align: center;
}
footer .xy_footer .footer_tools a:last-child {
  border-right: none;
  width: 40%;
}
footer .xy_footer .m_mmb {
  margin: 18px auto;
  text-align: center;
  color: #727272;
}
footer .xy_footer .m_mmb a {
  display: inline-block;
}
footer .xy_footer .m_mmb a span {
  color: #ff0000;
}
