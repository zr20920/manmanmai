header{
    position: relative;
    .xy_header {
        width: 100%;
        height: 50px;
        background-color: #ff841d;
        position: relative;
        .header_logo {
            width: 66%;
            position: absolute;
            top: 10px;
            left: 14px;
            a {
                img {
                    width: 185px;
                    height: 25px;
                }
            }
        }
        .header_app {
            position: absolute;
            top: 0;
            right: 0;
            a {
                padding: 6px;
                img {
                    width: 34px;
                    height: 34px;
                }
            }
        }
    }
    .xy_searchBar {
        width: 100%;
        height: 50px;
        border-bottom: 2px solid #f1f1f1;
        padding: 10px;
        .search-inner {
            width: 100%;
            height: 30px;
            position: relative;
            #searchText {
                width: 75vw;            
                height: 100%;
                background-color: #f1f1f1;
                border-radius: 15px;
                padding-left: 15px;
                // font-size: 14px;
                position: absolute;
                left: 0;
            }
            #searchBtn {
                width: 16.8vw;
                height: 100%;
                background-color: #ff841d;
                border-radius: 15px;
                // font-size: 16px;
                font-size: 4.75vw;            
                color: #fff;
                position: absolute;
                right: 0;
            }
        }
    }
}
main.section1{
    padding: 2vw;
    background-color: rgb(237, 237, 237);
    >ul{
        width: 100%;
        border-radius: 2vw;
        overflow: hidden; 
        background-color: rgb(227, 227, 227);
        border: 1px solid rgb(219, 219, 219);  
        li{
            border-bottom: 1px solid rgb(219, 219, 219);
            h3{ 
                line-height: 12.5vw;
                height: 100%;
                padding: 0 3.125vw;
                font-size: 5vw;
                background-color: rgb(237, 237, 237);
                // border-bottom: 1px solid rgb(219, 219, 219);
                span.fa{
                    float: right;
                    line-height: 12.5vw;
                    color: rgb(219, 219, 219);
                }
            }
            ul{ 
                display: none;
                overflow: hidden;
                background-color:rgb(249, 249, 249);
                border-bottom: 2px solid #999;
                li{
                    float: left;
                    width: 33.33%;
                    font-size: 3.75vw; 
                    text-align: center;
                    line-height: 12.5vw;
                    border-bottom: 1px solid rgb(219, 219, 219);
                    border-right: 1px solid rgb(219, 219, 219);
                    &:nth-child(3n){
                      border-right: none;  
                    }
                }
            }
        }
    }
}
main.section2{
    display: none;
    margin: 0vw auto;
    width: 100%;
    position: relative;
    .topBar{
        height: 12.5vw;
        padding: 0 2vw;
        background: linear-gradient(to bottom, #fff 0% , #eee 100%);
        border-top: 2px solid #aaa;
        a{
            height: 100%;
            line-height: 12.5vw;
            float: left;
            margin-right: 1vw; 
        }
        button{
            position: absolute;
            right: 2vw;
            background: linear-gradient(to top, #fff 0% , #ccc 100%);
            // box-shadow: 0 1px 1px 0 #ccc;
            border: 1px solid rgb(219, 219, 219);
            padding: 0 4vw;
            margin: 1.25vw 0;
            height: 10vw;
            border-radius: 2vw;
        }

    }
    .content{
        ul{
            li{
                height: 30vw;
                border-bottom: 1px solid rgb(219, 219, 219);
                >div{
                    width: 100%;
                    margin: 0 auto;
                }
                a{
                    width: 30%;
                    height: 100%;
                    float: left;
                    img{
                       width: 80%;
                       margin: 0 auto;
                       height: 100%;
                    }
                }
                .cont{  
                    padding: 2.5vw 0;
                    height: 100%;
                    p{
                        line-height: 6vw;
                        max-height: 12vw;
                        overflow: hidden;
                        padding: 0 2vw;
                    }
                    .price{
                        color: red;
                    }
                    span{
                        font-size: 3.75vw;
                        color: #ccc;
                        padding: 0 2vw;
                    }
                }
            }
        }
    }
    .pageBar{
        height: 12.5vw;
        padding: 2vw 30vw;
        position: relative;

        select{
            display: block;
            margin: 0 auto;
            height: 100%;
            width: 60%;
            font-size: 5vw;
        }
        button{
            position: absolute;
            top: 0;
            background: linear-gradient(to top, #eee 0% , #ccc 100%);
            box-shadow: 0 1px 1px 0 #ccc;
            border: 1px solid rgb(219, 219, 219);
            padding: 0 4vw;
            margin: 1.5vw 0;
            height: 70%;
            border-radius: 2vw;
            &:nth-of-type(1){
                left: 4vw;
            }
            &:nth-of-type(2){
                right: 4vw;
            }
        }
    }
}
main.section3{
    display: none;
    margin: 0vw auto;
    width: 100%;
    overflow: hidden;
    position: relative;
    .topBar{
        height: 12.5vw;
        padding: 0 2vw;
        background: linear-gradient(to bottom, #fff 0% , #eee 100%);
        border-top: 2px solid #aaa;
        a{
            height: 100%;
            line-height: 12.5vw;
            float: left;
            margin-right: 1vw; 
        }
        button{
            position: absolute;
            right: 2vw;
            background: linear-gradient(to top, #fff 0% , #ccc 100%);
            // box-shadow: 0 1px 1px 0 #ccc;
            border: 1px solid rgb(219, 219, 219);
            padding: 0 4vw;
            margin: 1.25vw 0;
            height: 10vw;
            border-radius: 2vw;
        }

    }
    .content{
        .pro{
            .p_t{
                position: relative;
                h3{
                    text-align: center;
                    margin-top: 5vw;
                }
                img{
                    width: 50%;
                    height: 50%;
                    margin: 0 auto;
                }
                span{
                    position: absolute;
                    bottom: 5vw;
                    right: 10vw;
                    padding: 2vw 0.8vw;
                    font-size: 3.75vw;
                    color: #fff;
                    border-radius: 50%;
                    background-color: rgb(139, 201, 0);
                }
            }
            table{
                border-bottom: 1px solid rgb(219, 219, 219);
                // border-top: 1px solid rgb(219, 219, 219);
                img{
                    width: 50%;
                    height: 50%;
                    margin: 2vw auto;
                }
            }
            >p{
                padding: 3vw;
            }
        }
        .comp{
            h3{
                background: linear-gradient(to top, #eef 0% , #ddd 100%);
                text-align: center;
                height: 12.5vw;
                line-height: 12.5vw;
            }
            >div{
                ul{
                    li{
                        >div{
                            color: rgb(193, 193, 193);
                            font-size: 3.75vw;
                            overflow: hidden;
                            padding: 3vw;
                            span{
                                float: left;
                            }
                            >div{
                                float: right;
                                p{
                                    text-align: right;
                                }
                            }
                        }
                        >p{
                            padding: 0 3vw 3vw;
                            border-bottom: 1px solid rgb(219, 219, 219);
                        }
                    }
                }
            }
        }
    }
    .pageBar{
        height: 20vw;
        button{
            background: linear-gradient(to top, #eee 0% , #ccc 100%);
            box-shadow: 0 1px 1px 0 #ccc;
            border: 1px solid rgb(219, 219, 219);
            margin: 4vw 10%;
            font-size: 5vw;
            height: 50%;
            width: 80%;
            border-radius: 2vw;
        }
    }
}
footer{
    .xy_footer {
        width: 100%;
        height: 122px;
        background-color: #efefef;
        padding-top: 20px;
        .footer_tools {
            height: 32px;
            border-top: 1px solid #c4c4c4;
            border-bottom: 1px solid #c4c4c4;
            a {
                float: left; // width:1.12rem;
                width: 30%;
                height: 100%;
                border-right: 1px solid #c4c4c4;
                line-height: 32px;
                text-align: center;
            }
            a:last-child {
                border-right: none;
                width: 40%;
            }
        }
        .m_mmb {
            margin: 18px auto;
            text-align: center;
            color: #727272;
            a {
                display: inline-block;
                span {
                    color: #ff0000;
                }
            }
        }
    }
}