html,
body {
  width: 100%;
  height: 100%;
}
* {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
  box-sizing: border-box;
}
ul {
  list-style: none;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  outline: none;
  border: none;
}
img {
  display: block;
  width: 100%;
}
.clearfix::after {
  content: "";
  display: block;
  height: 0;
  clear: both;
}
