<!--定义模板01-->

<script type="text/html" id="template">
    <li id='item{{indexmenuId}}'>
        <a href="{{titlehref}}">
            {{@img}}
            <p>{{name}}</p>
        </a>
    </li>
</script>

<!--定义模板02-->

<script type="text/html" id="template02">
    <li id="{{productId}}">
        <a href="./Savinginfor.html?id={{productId}}">
            {{@productImgSm}}
        </a>
        <a href="javascript:void(0)">
            {{productName}}
            <span>{{productPinkage}}</span>
        </a>
        <div>
            <p>{{productFrom}}</p> <span>|</span>
            <p>{{productTime}}</p>
        </div>
        <span>{{productComCount}}</span>
    </li>
</script>