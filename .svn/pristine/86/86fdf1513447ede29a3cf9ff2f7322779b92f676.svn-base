header {
  position: relative;
  top: 0;
  width: 100%;
  height: 25vw;
  background-color: orange;
}
main {
  margin: 0vw auto;
  width: 100%;
  overflow: hidden;
  position: relative;
}
main .topBar {
  height: 12.5vw;
  padding: 0 2vw;
  background: linear-gradient(to bottom, #fff 0%, #eee 100%);
  border-top: 2px solid #aaa;
}
main .topBar a {
  height: 100%;
  line-height: 12.5vw;
  float: left;
  margin-right: 1vw;
}
main .topBar button {
  position: absolute;
  right: 2vw;
  background: linear-gradient(to top, #fff 0%, #ccc 100%);
  border: 1px solid #dbdbdb;
  padding: 0 4vw;
  margin: 1.25vw 0;
  height: 10vw;
  border-radius: 2vw;
}
main .content .pro .p_t {
  position: relative;
}
main .content .pro .p_t h3 {
  text-align: center;
  margin-top: 5vw;
}
main .content .pro .p_t img {
  width: 50%;
  height: 50%;
  margin: 0 auto;
}
main .content .pro .p_t span {
  position: absolute;
  bottom: 5vw;
  right: 10vw;
  padding: 2vw 0.8vw;
  font-size: 3.75vw;
  color: #fff;
  border-radius: 50%;
  background-color: #8bc900;
}
main .content .pro table {
  border-bottom: 1px solid #dbdbdb;
}
main .content .pro table img {
  width: 50%;
  height: 50%;
  margin: 2vw auto;
}
main .content .pro > p {
  padding: 3vw;
}
main .content .comp h3 {
  background: linear-gradient(to top, #eef 0%, #ddd 100%);
  text-align: center;
  height: 12.5vw;
  line-height: 12.5vw;
}
main .content .comp > div ul li > div {
  color: #c1c1c1;
  font-size: 3.75vw;
  overflow: hidden;
  padding: 3vw;
}
main .content .comp > div ul li > div span {
  float: left;
}
main .content .comp > div ul li > div > div {
  float: right;
}
main .content .comp > div ul li > div > div p {
  text-align: right;
}
main .content .comp > div ul li > p {
  padding: 0 3vw 3vw;
  border-bottom: 1px solid #dbdbdb;
}
main .pageBar {
  height: 20vw;
}
main .pageBar button {
  background: linear-gradient(to top, #eee 0%, #ccc 100%);
  box-shadow: 0 1px 1px 0 #ccc;
  border: 1px solid #dbdbdb;
  margin: 4vw 10%;
  font-size: 5vw;
  height: 50%;
  width: 80%;
  border-radius: 2vw;
}
footer {
  width: 100%;
  height: 12.5vw;
  background-color: orange;
}
